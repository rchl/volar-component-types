{
  "name": "volar-component-types",
  "version": "0.4.0",
  "description": "A module that provides Volar types for auto-discovered components in Nuxt 2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rchl/volar-component-types.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "type": "module",
  "main": "dist/volar.cjs",
  "exports": {
    ".": {
      "import": "./dist/volar.mjs",
      "require": "./dist/volar.cjs"
    },
    "./nuxt": {
      "types": "./dist/module.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "nuxt": [
        "./dist/module.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxt dev -c ./fixture/nuxt.config.ts",
    "build": "unbuild",
    "lint": "eslint --ext .js,.ts,.vue .",
    "typecheck": "tsc && vue-tsc",
    "prepare": "pnpm build",
    "release": "release-it"
  },
  "author": "Rafal Chlodnicki",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.23.5",
    "@babel/traverse": "^7.23.5",
    "@volar/language-core": "^1.11.1",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@nuxt/types": "^2.17.2",
    "@release-it/conventional-changelog": "^8.0.1",
    "@types/babel__traverse": "^7.20.4",
    "@types/eslint": "^8.44.8",
    "@types/node": "^18.19.0",
    "eslint": "^8.54.0",
    "eslint-config-rchl-typescript": "^2.0.1",
    "eslint-config-rchl-vue": "^2.0.0",
    "globby": "^14.0.0",
    "lefthook": "^1.5.5",
    "nuxt": "^2.17.2",
    "release-it": "^17.0.0",
    "sass": "^1.69.5",
    "typescript": "^5.3.2",
    "unbuild": "^2.0.0",
    "vue": "^2.7.15",
    "vue-client-only": "^2.1.0",
    "vue-router": "^3.6.5",
    "vue-tsc": "^1.8.24",
    "vuex": "^3.6.2"
  }
}
