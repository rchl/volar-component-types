{
  "name": "volar-component-types",
  "version": "1.0.0",
  "description": "A module that provides Volar types for auto-discovered components in Nuxt 2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rchl/volar-component-types.git"
  },
  "packageManager": "pnpm@9.12.1",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "type": "module",
  "main": "./dist/main.cjs",
  "exports": {
    ".": {
      "types": "./dist/main.d.ts",
      "import": "./dist/main.mjs",
      "require": "./dist/main.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nuxt dev -c ./fixture/nuxt.config.ts",
    "build": "unbuild",
    "lint": "eslint .",
    "typecheck": "tsc && vue-tsc",
    "prepare": "pnpm build",
    "release": "release-it"
  },
  "author": "Rafal Chlodnicki",
  "license": "MIT",
  "dependencies": {
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@nuxt/types": "^2.18.1",
    "@release-it/conventional-changelog": "^9.0.0",
    "@types/node": "^20.16.5",
    "chokidar": "3",
    "eslint": "^9.12.0",
    "eslint-config-rchl-base": "^2.0.4",
    "eslint-config-rchl-typescript": "^3.0.3",
    "eslint-config-rchl-vue": "^3.0.1",
    "globby": "^11.1.0",
    "lefthook": "^1.7.15",
    "nuxt": "^2.18.1",
    "release-it": "^17.7.0",
    "sass": "^1.79.2",
    "typescript": "^5.6.2",
    "unbuild": "^2.0.0",
    "vue": "^2.7.16",
    "vue-client-only": "^2.1.0",
    "vue-router": "^3.6.5",
    "vue-tsc": "^2.1.6",
    "vuex": "^3.6.2"
  },
  "pnpm": {
    "packageExtensions": {
      "@nuxt/types": {
        "peerDependencies": {
          "chokidar": "^3.5.3",
          "globby": "^11.1.0"
        }
      }
    }
  }
}
